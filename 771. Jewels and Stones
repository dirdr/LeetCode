class Solution {
    public int numJewelsInStones(String J, String S) {
        int result = 0;
        for(int i = 0; i < J.length(); i++){
            for(int y = 0; y < S.length(); y++){
                if(J.charAt(i) == S.charAt(y))
                    result++;
            }
        }
        return result;
    }
}
